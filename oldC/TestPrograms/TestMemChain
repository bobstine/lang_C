		VECTOR test1, test2, test3;		AllocVector(5, &test1);	CheckMemPointer ((char*)test1.pntr, "Should be in location 1");	AllocVector (7, &test2);	AllocVector (10, &test3);	PrintMemChain();	DisposeVector (&test2);	PrintMemChain();	DisposeVector (&test1);	PrintMemChain();	DisposeVector (&test3);	PrintMemChain();	DisposeVector (&test1);	CheckMemPointer ((char *)test1.pntr, "Should NOT be found in empty list");		Wait();	